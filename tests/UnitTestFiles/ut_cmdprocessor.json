          },
          {
            "SaveStrVars": [
              {"Params": [0,0],"Output": {"Name": ["SaveStrVars"]}}
            ]
          },
          {
            "LogTor": [
              {"Params": [0,0],"Output": {"Name": ["LogBigCore"]}}
            ]
          }
        ]
      }
